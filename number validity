%{
#include <stdio.h>
%}

digit   [0-9]
mobnum  ^{digit}{10}$

%%

{mobnum}        { printf("Valid mobile number: %s\n", yytext); }
{digit}+        { printf("Invalid mobile number: %s\n", yytext); }
.|\n            { /* ignore other characters */ }

%%

int main(int argc, char **argv)
{
    yylex();
    return 0;
}

int yywrap()
{
    return 1;
}

